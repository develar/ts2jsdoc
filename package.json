{
  "name": "ts-jsdoc",
  "description": "Transform TypeScript to JSDoc annotated JS code",
  "version": "4.0.0",
  "license": "MIT",
  "bin": {
    "ts2jsdoc": "out/ts2jsdoc.js"
  },
  "main": "out/ts2jsdoc.js",
  "files": [
    "out",
    "*.js"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "release": "pnpm compile && pnpm publish --no-git-checks",
    "test": "pnpm compile && jest"
  },
  "dependencies": {
    "bluebird-lst": "^1.0.9",
    "chalk": "^4.1.1",
    "doctrine": "^3.0.0",
    "fs-extra": "^10.0.0",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@types/doctrine": "^0.0.3",
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^20.3.2",
    "jest": "^26.6.3",
    "jest-environment-node-debug": "^2.0.0",
    "typescript": "^4.2.4 || ^5.0.0"
  },
  "peerDependencies": {
    "typescript": "^4.2.4 || ^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/develar/ts2jsdoc.git"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "typescript",
    "jsdoc"
  ],
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "test"
    ],
    "transformIgnorePatterns": [
      "<rootDir>"
    ],
    "testPathIgnorePatterns": [
      "[\\/]{1}fixtures[\\/]{1}"
    ],
    "testRegex": "\\.js$"
  }
}
